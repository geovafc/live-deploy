
/**
 * The Employee entity.
 */
entity Produto {
	nome String,
	valor BigDecimal,
	descricao String
}


// Set pagination options
paginate Produto with infinite-scroll

// Use Data Transfer Objects (DTO)
 dto * with mapstruct

// Set service options to all except few
service Produto with serviceImpl


